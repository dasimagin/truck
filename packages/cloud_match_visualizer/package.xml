<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>cloud_match_visualizer</name>
  <version>0.2.0</version>
  <description>A demo package that was developed as a way to explore the possibilities of underlying libraries.</description>
  <maintainer email="depressingsupremacy@outlook.com">Isa Gadaev</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>PCL 1.10</depend>
  <depend>pcl_conversions</depend>
  <depend>rclcpp</depend>
  <depend>rmw</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>
  <depend>rosbag2_cpp</depend>
  <depend>rosbag2_storage</depend>
  <depend>laser_geometry</depend>
  <depend>common_interfaces</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
